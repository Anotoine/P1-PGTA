                                <TextBlock Text="Callsign: " FontWeight="Bold" Grid.Row="3" Margin="5" Foreground="{Binding Path=TableTextColor, Converter={StaticResource UniversalValueConverter}}"/>
                                <TextBlock Text="{Binding Callsign}" Grid.Column="1" Grid.Row="3" Margin="5" Foreground="{Binding Path=TableTextColor, Converter={StaticResource UniversalValueConverter}}"/>
                                <TextBlock Text="Register ID: " FontWeight="Bold" Grid.Row="4" Grid.Column="0" Margin="5" Foreground="{Binding Path=TableTextColor, Converter={StaticResource UniversalValueConverter}}"/>
