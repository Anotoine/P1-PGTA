                                        <Button Name="MoreInfo" Content="?" Click="MDInfo_Click" Width="15" Height="18" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                                    </Grid>
                                </DataTemplate>
                            </DataGridTemplateColumn.HeaderTemplate>

                            <DataGridTemplateColumn.CellTemplate>
