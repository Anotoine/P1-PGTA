        <GroupBox x:Name="WPerformance" Grid.ColumnSpan="6" Grid.Row="1" Visibility="Hidden" BorderThickness="0">
            <StackPanel HorizontalAlignment="Center" Margin="25">
                <DataGrid x:Name="TPerfo" CanUserAddRows="False" CanUserDeleteRows="False" AutoGenerateColumns="False" HorizontalContentAlignment="Stretch" SelectionMode="Single" SelectionUnit="FullRow" Background="Transparent" AlternatingRowBackground="{Binding Path=TableSecondaryColor, Converter={StaticResource UniversalValueConverter}}" GridLinesVisibility="Vertical" RowBackground="{Binding Path=TableMainColor, Converter={StaticResource UniversalValueConverter}}">
