                                            <ColumnDefinition Width="100"/>
                                            <ColumnDefinition Width="100"/>
                                        </Grid.ColumnDefinitions>
                                        <TextBlock Text="{Binding ProbOfUpdate}" HorizontalAlignment="Center" Grid.Column="0" Foreground="{Binding Path=TableTextColor, Converter={StaticResource UniversalValueConverter}}"/>
                                        <TextBlock Text="{Binding PoUDTeo}" HorizontalAlignment="Center" Grid.Column="1" Foreground="{Binding Path=TableTextColor, Converter={StaticResource UniversalValueConverter}}"/>
                                    </Grid>
                                </DataTemplate>
                            </DataGridTemplateColumn.CellTemplate>
