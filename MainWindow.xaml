<Window x:Class="P1_PGTA.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:P1_PGTA"
        mc:Ignorable="d"
        Title="Project 1" Height="450" Width="800" Background="Black">
    <Grid x:Name="Base" Background="Black">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="48*"/>
            <ColumnDefinition Width="349*"/>
        </Grid.ColumnDefinitions>
        <Button Name="Button" Content="Button" HorizontalAlignment="Left" Margin="7,200,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click" Background="#FF00F3FF" Grid.Column="1" Height="20"/>
        <TextBox Name="TextBoxList" AcceptsReturn="True" TextWrapping="Wrap" Grid.Column="1" Margin="96,16,32,19" Background="#FF20C7C7"/>

    </Grid>
</Window>
